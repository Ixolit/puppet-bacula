JobDefs {
    Name     = <%= @name %>
    Type     = <%= @jobtype %>
    Pool     = <%= @pool %>
    Schedule = <%= @sched %>
    Messages = <%= @messages %>
    Priority = <%= @priority %>
    Accurate = <%= @accurate ? 'yes' : 'no' %>
<% if @level -%>
    Level    = <%= @level %>
<% end -%>
<% if @max_concurrent_jobs -%>
    Maximum Concurrent Jobs = <%= @max_concurrent_jobs %>
<% end -%>
<%= scope.function_template(['bacula/_job_reschedule.erb']) -%>
}
